@model PaginatedModel<ContestSubmissionViewModel>

<div class="gallery_area clearfix">
    @if (TempData["success"] != null)
    {
    <div class="alert alert-success">
        <strong>Success!</strong> @TempData["success"]
    </div>
    }
    <div class="container-fluid clearfix">
        <h4 class="text-success">Reviewed</h4>
        <hr>
        <div class="row portfolio-column">

            @foreach (var item in Model.Model)
            {
                @if (item.HasJuryGivenSubmission)
                {
                    <partial name="~/Views/Shared/Partials/_ContestSubmissionPartial.cshtml" model="item" />
                }
            }
        </div>
        <br>
        <br>
        <h4 class="text-danger">Not Reviewed</h4>
        <hr>
        <div class="row portfolio-column">
            @foreach (var item in Model.Model)
            {
                @if (!item.HasJuryGivenSubmission)
                {
                    <partial name="~/Views/Shared/Partials/_ContestSubmissionPartial.cshtml" model="item" />
                }
            }
        </div>
        <div class="row">
            <div class="col-12 text-center mt-70">
                <a href="#" class="btn studio-btn"><img src="/img/core-img/logo-icon.png" alt=""> Load More</a>
            </div>
        </div>
    </div>
</div>